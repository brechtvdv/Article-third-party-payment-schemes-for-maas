main {
  counter-reset: section;

  // Sections
  h2 {
    counter-reset: subsection;

    &::before {
      counter-increment: section;
      content: counter(section) ". ";
    }
  }

  // Subsections
  h3 {
    &::before {
      counter-increment: subsection;
      content: counter(section) "." counter(subsection) ". ";
    }
  }

  // Paragraphs
  h4 {
    margin: 0 1em 0 0;
    float: left;
    clear: left;
  }

  // Links
  a {
    // Display link text
    &[data-link-text] {
      &:after {
        content: ' (' attr(data-link-text) ')';
        font-style: italic;
      }
    }
